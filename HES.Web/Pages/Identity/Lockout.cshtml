@page "/lockout"
@model HES.Web.Pages.Identity.LockoutModel
@{
    ViewData["Title"] = Resource.Identity_Lockout_Title;
}

<div class="row h-100 justify-content-center align-items-center">
    <div class="text-center">
        <h1 class="text-danger">@ViewData["Title"]</h1>
        <p class="text-danger">@Resource.Identity_Lockout_Description</p>
        <a href="@Routes.Login">@Resource.Label_GoToLoginPage</a>
    </div>
</div>
